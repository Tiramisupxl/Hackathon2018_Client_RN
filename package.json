{
  "name": "Hackathon2018_Client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "fstart": "react-native start --reset-cache",
    "rnlink": "react-native link",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "format": "prettier-standard 'src/**/*.{ts,tsx}'",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "linters": {
      "src/**/*.{ts,tsx}": [
        "prettier-standard",
        "git add"
      ]
    }
  },
  "dependencies": {
    "@types/react": "^16.4.13",
    "@types/react-native": "^0.56.15",
    "@types/react-navigation": "^2.0.19",
    "appcenter": "^1.8.0",
    "appcenter-analytics": "^1.8.0",
    "appcenter-crashes": "^1.8.0",
    "react": "16.4.2",
    "react-native": "0.56.0",
    "react-native-splash-screen": "^3.1.1",
    "react-navigation": "^2.12.1"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/react-test-renderer": "^16.0.2",
    "babel-jest": "23.4.2",
    "babel-preset-react-native": "^5.0.0",
    "husky": "^0.14.3",
    "jest": "23.5.0",
    "lint-staged": "^7.2.2",
    "prettier-standard": "^8.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.4.2",
    "ts-jest": "^23.1.4",
    "typescript": "^3.0.3"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache"
  }
}