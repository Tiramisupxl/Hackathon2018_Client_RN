{
  "name": "Hackathon2018_Client",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "fstart": "react-native start --reset-cache",
    "rnlink": "react-native link",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "format": "prettier-standard 'src/**/*.{ts,tsx}'",
    "test": "jest"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "prettier-standard",
      "git add"
    ]
  },
  "dependencies": {
    "appcenter": "^1.8.1",
    "appcenter-analytics": "^1.8.1",
    "appcenter-crashes": "^1.8.0",
    "react": "16.5.2",
    "react-native": "0.57.1",
    "react-native-splash-screen": "^3.1.1",
    "react-navigation": "^2.17.0",
    "react-navigation-fluid-transitions": "^0.2.72"
  },
  "devDependencies": {
    "@types/jest": "^23.3.2",
    "@types/react-test-renderer": "^16.0.2",
    "@types/react": "^16.4.14",
    "@types/react-native": "^0.57.0",
    "@types/react-navigation": "^2.0.23",
    "babel-jest": "23.6.0",
    "babel-preset-react-native": "^5.0.0",
    "husky": "^1.1.0",
    "jest": "23.6.0",
    "lint-staged": "^7.3.0",
    "prettier-standard": "^8.0.1",
    "react-addons-test-utils": "^15.6.2",
    "react-native-typescript-transformer": "^1.2.10",
    "react-test-renderer": "16.5.2",
    "ts-jest": "^23.10.3",
    "typescript": "^3.1.1"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest",
      "\\.(ts|tsx)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "testPathIgnorePatterns": [
      "\\.snap$",
      "<rootDir>/node_modules/",
      "<rootDir>/lib/"
    ],
    "cacheDirectory": ".jest/cache",
    "setupFiles": [
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js"
    ]
  }
}